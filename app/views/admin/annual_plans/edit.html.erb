<div class="container">
  <div class="row">
    <div class="col mt-5">
      <h2><%= @annual_plan.created_at.year %>年 年間指導計画</h2>
      <h2><%= @group.age %> 歳児 <%= @group.name %></h2>
      <%= form_with model: @annual_plan, url: admin_group_annual_plan_path(@annual_plan), method: :patch , local: true do |f| %>
        <%= render partial: "form", locals: { f: f } %>
      <% end %>
    </div>
  </div>
</div>

